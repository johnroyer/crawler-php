{
    "name": "johnroyer/crawler-php",
    "description": "crawler implement in PHP",
    "type": "library",
    "require": {
        "php": "^8.1|^8.2",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.5",
        "symfony/dom-crawler": "^6.2",
        "symfony/css-selector": "^6.2",
        "glenscott/url-normalizer": "^1.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Zeroplex\\Crawler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/src/"
        }
    },
    "authors": [
        {
            "name": "Zero",
            "email": "john.royer@gmail.com"
        }
    ],
    "minimum-stability": "stable"
